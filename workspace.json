            "baseUrl": "http://localhost:4200/",
            "devServerTarget": "multi-cart:serve",
            "exit": false,
            "record": false,
            "skipServe": true,
            "watch": true
          },
          "configurations": {
            "production": {
              "devServerTarget": "multi-cart:serve:production"
            }
