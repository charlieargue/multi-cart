{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "defaultProject": "multi-cart",
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "mock-api": {
      "root": "libs/mock-api",
      "sourceRoot": "libs/mock-api/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/mock-api/**/*.ts"]
          }
        }
      }
    },
    "multi-cart": {
      "root": "apps/multi-cart",
      "sourceRoot": "apps/multi-cart",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/multi-cart",
            "outputPath": "dist/apps/multi-cart"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "multi-cart:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "multi-cart:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "multi-cart:build:production"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/multi-cart/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
